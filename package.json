{
  "name": "roundmanagerapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prebuild": "node scripts/update-build-id.js && node scripts/copy-assets.js",
    "lint": "expo lint",
    "build": "npm run build:marketing && npm run build:app",
    "build:marketing": "cd web && npm install && npm run build",
    "build:app": "expo export --platform web --output-dir dist",
    "postbuild": "node scripts/merge-builds.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@quidone/react-native-wheel-picker": "^1.4.1",
