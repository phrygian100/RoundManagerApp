    "expo-web-browser": "~14.2.0",
    "@expo/metro-config": "~0.20.0",
    "firebase": "^9.22.2",
    "papaparse": "^5.5.3",
    "react": "19.0.0",
    "react-datepicker": "^8.4.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-print": "^0.11.0",
    "react-native-reanimated": "~3.17.4",
