    "@types/papaparse": "^5.3.16",
    "@types/react": "~19.0.10",
    "@types/stripe": "^8.0.416",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3",
    "typescript-transform-paths": "^3.5.5"
